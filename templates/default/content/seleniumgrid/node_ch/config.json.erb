{
  "capabilities": [
    {
      "version": "<%= `/usr/bin/google-chrome -version | awk '{ print $3 }'`.strip %>",
      "browserName": "chrome",
      "maxInstances": <%= node['aet']['seleniumgrid']['node_ch']['NODE_MAX_INSTANCES'] %>,
      "seleniumProtocol": "WebDriver",
      "applicationName": ""
    }
  ],
  "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
  "maxSession": <%= node['aet']['seleniumgrid']['node_ch']['NODE_MAX_SESSION'] %>,
  "host": "<%= node['aet']['seleniumgrid']['node_ch']['NODE_HOST'] %>",
  "port": <%= node['aet']['seleniumgrid']['node_ch']['NODE_PORT'] %>,
  "register": true,
  "registerCycle": <%= node['aet']['seleniumgrid']['node_ch']['NODE_REGISTER_CYCLE'] %>,
  "nodePolling": <%= node['aet']['seleniumgrid']['node_ch']['NODE_POLLING'] %>,
  "unregisterIfStillDownAfter": <%= node['aet']['seleniumgrid']['node_ch']['NODE_UNREGISTER_IF_STILL_DOWN_AFTER'] %>,
  "downPollingLimit": <%= node['aet']['seleniumgrid']['node_ch']['NODE_DOWN_POLLING_LIMIT'] %>
}
